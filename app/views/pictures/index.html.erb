<h1>Pictures</h1>

<!--a href="/pictures/new">New Picture</a-->
<!-- <a href="<%= new_picture_path %>">New Picture</a> -->
<%= link_to "New Picture", new_picture_path %>

<% @pictures.each do |picture| %>
  <h2><%= picture.title %></h2>
  <%= button_to "Edit", edit_picture_path(picture), :method => :get%>
  <!-- <a href="<%= edit_picture_path(picture) %>">Edit</a> -->
  <p><%= picture.artist %></p>
  <!-- <a href="<%= picture_path(picture)%>"> -->
    <!-- <img src="<%= picture.url %>"> -->
  <!-- </a> -->
  <%= link_to picture_path(picture) do %>
    <img src="<%= picture.url%>">
  <% end %>
<% end %>

